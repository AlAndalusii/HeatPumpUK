<svg viewBox="0 0 32 32" fill="none" xmlns="http://www.w3.org/2000/svg">
  <!-- Simplified favicon for ICO format - optimized for small sizes -->
  <rect width="32" height="32" fill="white"/>
  
  <g>
    <!-- Outer circle - subtle background -->
    <circle 
      cx="16" 
      cy="16" 
      r="14" 
      fill="#e8f4ff" 
      opacity="0.5"
    />
    
    <!-- Main icon shape - three-quarter circle representing continuous energy flow -->
    <circle 
      cx="16" 
      cy="16" 
      r="10" 
      stroke="#0071e3" 
      stroke-width="2"
      stroke-linecap="round"
      fill="none"
      stroke-dasharray="47 16"
      stroke-dashoffset="0"
      transform="rotate(-90 16 16)"
    />
    
    <!-- Inner accent circle - represents core efficiency -->
    <circle 
      cx="16" 
      cy="16" 
      r="6" 
      stroke="#34c759" 
      stroke-width="1.5"
      stroke-linecap="round"
      fill="none"
      stroke-dasharray="28 9"
      stroke-dashoffset="0"
      transform="rotate(45 16 16)"
    />
    
    <!-- Center dot - focal point -->
    <circle 
      cx="16" 
      cy="16" 
      r="2" 
      fill="#0071e3"
    />
    
    <!-- Elegant arrow indicator - subtle directional element -->
    <path
      d="M 20 11 L 22.5 8.5 L 20 6"
      stroke="#34c759"
      stroke-width="1.5"
      stroke-linecap="round"
      stroke-linejoin="round"
      fill="none"
    />
  </g>
</svg>


